/* pages/seeuser/seeuser.wxss */
page{
  background-color: #FFFFFF;
}
.head{
  display: flex;
  flex-direction: row;
  margin-left: 80rpx;
}
.head input{
  border: 1rpx gray solid;
  border-radius: 10rpx;
  min-height: 80rpx;
}
.head text{
  margin-left: 10rpx;
  font-size: 50rpx;
}
.lalalal{
  display: flex;
  flex-direction: row;
}
.lalalal image{
  width: 65rpx;
  height: 65rpx;
  margin-left: 20rpx;
}
.body{
  display: flex;
  flex-direction: column;
  background-color: #D3D3D3;
  margin-left: 15%;
  margin-right: 15%;
  margin-top: 100rpx;
  border-radius: 20rpx;
}
.body text{
  padding-top: 10rpx;
  padding-left: 15rpx;
}
.anniu{
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
  margin-left: 100rpx;
  margin-right: 100rpx;
}
.btn1 {
  width: 30%;
  margin-top: 20rpx;
  background-color: #888;
  color: #FFFFFF;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.btnImg {
  margin-right: 8rpx;
  width: 46rpx;
  height: 46rpx;
}

.btn1::after {
  border-radius: 98rpx;
  border: 0; 
}